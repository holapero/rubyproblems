a = ["Aaron","Mike","Kelli"]

def hash_maker(a)
    result = {}
    a.each do |element|
        result[element] = 0
    end
    return result
end

hash_maker(a)




2)


a = [1,2,3,"string",4,5,6]

def sum(arr)
    i = 0 
    result = 0 
    while i < arr.length do 
        if arr[i].class == String
            arr[i] = 0
        end
        result += arr[i]
        i += 1
    end
    return result 
end

sum(a)






